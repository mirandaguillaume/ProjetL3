tag:
    pattern:  /
    defaults: { _controller: "ProjetL3TagBundle:Tag:index" }

tag_show:
    pattern:  /{id}/show
    defaults: { _controller: "ProjetL3TagBundle:Tag:show" }

tag_new:
    pattern:  /new
    defaults: { _controller: "ProjetL3TagBundle:Tag:new" }

tag_create:
    pattern:  /create
    defaults: { _controller: "ProjetL3TagBundle:Tag:create" }
    requirements: { _method: post }

tag_edit:
    pattern:  /{id}/edit
    defaults: { _controller: "ProjetL3TagBundle:Tag:edit" }

tag_update:
    pattern:  /{id}/update
    defaults: { _controller: "ProjetL3TagBundle:Tag:update" }
    requirements: { _method: post|put }

tag_delete:
    pattern:  /{id}/delete
    defaults: { _controller: "ProjetL3TagBundle:Tag:delete" }
    requirements: { _method: post|delete }
