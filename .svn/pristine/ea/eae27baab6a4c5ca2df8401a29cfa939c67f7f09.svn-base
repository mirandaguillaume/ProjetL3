ProjetL3\LienBundle\Entity\Lien:
    type: entity
    table: null
    repositoryClass: ProjetL3\LienBundle\Entity\LienRepository
    fields:
        id:
            type: integer
            id: true
            generator:
                strategy: AUTO
        nom:
            type: string
            length: 255
            nullable: false
        url:
            type: string
            length: 255
            nullable: false
        date:
            type: datetime
            nullable: false
        nb_votes:
            type: integer
            nullable: false
        nb_clics:
            type: integer
            nullable: false

    manyToMany:
        users:
            targetEntity: ProjetL3\UserBundle\Entity\User
            mappedBy: liens
        tags:
            targetEntity: ProjetL3\TagBundle\Entity\Tag
            mappedBy: liens
        listes:
            targetEntity: ProjetL3\ListeBundle\Entity\Liste
            mappedBy : liens


    uniqueConstraints:
            unique_credential:
                columns: [ nom ]

    lifecycleCallbacks: {  }
