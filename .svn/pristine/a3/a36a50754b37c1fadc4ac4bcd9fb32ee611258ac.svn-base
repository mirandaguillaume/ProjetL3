ProjetL3\UserBundle\Entity\User:
    type:  entity
    table: fos_user
    id:
        id:
            type: integer
            generator:
                strategy: AUTO
    fields:
        nom:
            type: string
            length: 30
            nullable: false
        prenom:
            type: string
            length: 30
            nullable: false
        freq_actu:
            type: string
            values: ['mois', 'jour', 'null']